{
    "rewrites": [
      {
        "source": "/assets/",
        "destination": "/auth"  // Block directory access (redirect to 404)
      }
    ],
    "headers": [
      {
        "source": "/assets/*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=86400"  // Allow the cache to store assets
          }
        ]
      }
    ]
  }
  